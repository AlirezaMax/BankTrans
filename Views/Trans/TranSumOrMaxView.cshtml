@model IEnumerable<BankTr.Core.Domain.Tran>

    @{
        ViewBag.Title = "TranSumOrMaxView";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h2>Result</h2>
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>State</th>
                <th>Amount($)</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var tran in Model)
            {
                <tr>
                    <td>@tran.State.Code</td>
                    
                    <td>@tran.Amount</td>                

                </tr>
            }
        </tbody>
    </table>



